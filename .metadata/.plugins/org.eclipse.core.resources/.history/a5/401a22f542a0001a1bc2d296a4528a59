import java.util.Scanner;

public class Program2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int m = sc.nextInt();
		int n = sc.nextInt();
		
		int mat[][] = new int[m][n];
		
		for (int i = 0; i<mat.length; i++) {
			for (int j = 0; j<mat[i].length; j++) {
				mat[m][n] = sc.nextInt();
			}
		}
		
		System.out.println();
		
		int find = sc.nextInt();
		
		for (int i=0; i<mat.length; i++) {
			for (int j=0; j<mat[i].length; j++) {
				if(mat[m][n] == find) {
					System.out.printf("Position: %d,%d", mat[i][j]);
					System.out.printf("Left: %d", mat[i-1][j]);s
				}
			}
		}
		
		sc.close();
	}

}
