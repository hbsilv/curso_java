package java_project;

import java.util.Scanner;

public class udemy {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
	
		int N = sc.nextInt();
		int v1, v2, v3;
		for (int i=1; i<=N; i++) {
			v1 = i;
			v2 = i*i;
			v3 = i*i*i;
			System.out.println("%d %d %d", v1, v2, v3);
		}

	}
}